    "chokidar-cli": "^3.0.0",
    "colors": "^1.4.0",
    "concurrently": "^8.2.1",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "documentalist": "^1.6.0",
    "eslint": "~8.46.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.27.5",
